#ifndef SRC_HDR_I2C_H_
#define SRC_HDR_I2C_H_

#define AXI_I2C_GIE				( 0x4160001c )
#define AXI_I2C_ISR				( 0x4160001c )
#define AXI_I2C_IER				( 0x4160001c )
#define AXI_I2C_SOFTR			( 0x4160001c )
#define AXI_I2C_CR				( 0x4160001c )
#define AXI_I2C_SR				( 0x4160001c )
#define AXI_I2C_TX_FIFO			( 0x4160001c )
#define AXI_I2C_RX_FIFO			( 0x4160001c )
#define AXI_I2C_ADR				( 0x4160001c )
#define AXI_I2C_TX_FIFO_OCY		( 0x4160001c )
#define AXI_I2C_RX_FIFO_OCY		( 0x4160001c )
#define AXI_I2C_TEN_ADR			( 0x4160001c )
#define AXI_I2C_RX_FIFO_PIRQ	( 0x4160001c )
#define AXI_I2C_GPO				( 0x4160001c )
#define AXI_I2C_TSUSTA			( 0x4160001c )
#define AXI_I2C_TSUSTO			( 0x4160001c )
#define AXI_I2C_THDSTA			( 0x4160001c )
#define AXI_I2C_TSUDAT			( 0x4160001c )
#define AXI_I2C_THIGH			( 0x4160001c )
#define AXI_I2C_TBUF			( 0x4160001c )
#define AXI_I2C_TLOW			( 0x4160001c )
#define AXI_I2C_THDDAT			( 0x4160001c )


#endif /* SRC_HDR_I2C_H_ */
