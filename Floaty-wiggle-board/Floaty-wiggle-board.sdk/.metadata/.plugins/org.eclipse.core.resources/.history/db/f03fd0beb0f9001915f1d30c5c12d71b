#ifndef HDR_ERRORCODES_H_
#define HDR_ERRORCODES_H_

typedef enum
{
	setup_succes 	= 0x0,
	setup_fail 		= 0x1
}setup_status_t;

typedef enum
{
	program_status_succes 	= 0x00,
	program_status_generic	= 0x10,
	program_status_sensor	= 0x20,
	program_status_math	= 0x30,
	program_status_serial	= 0x40
}program_status_category_t;

typedef enum
{
	generic_status_unknown = 0x0
}program_status_generic_t;

typedef enum
{
	sensor_status_unknown = 0x0,
	sensor_status_bad_gyro_read = 0x1,
	sensor_status_bad_accel_read = 0x2
}program_status_sensor_t;

#endif /* HDR_ERRORCODES_H_ */
